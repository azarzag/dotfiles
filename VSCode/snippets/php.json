{
    "Method": {
        "prefix": "met",
        "body": [
            "public function $1($2)",
            "{",
            "    $3",
            "}"
        ]
    },

    "Test Method": {
        "prefix": "test",
        "body": [
            "/** @test */",
            "public function $1()",
            "{",
            "    $3",
            "}"
        ]
    },

    "Tinker": {
        "prefix": "tinker",
        "body": [
            "eval(\\Psy\\sh());"
        ]
    },

    "Die and Dump": {
        "prefix": "d",
        "body": [
            "dd($1);"
        ]
    },

    "Try Catch": {
        "prefix": "try",
        "body": [
            "try {",
            "$1",
            "} catch ($2) {",
            "$3",
            "}"
        ]
    }
}
